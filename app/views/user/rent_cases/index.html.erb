<p id="notice"><%= notice %></p>

<h1>Listing Rent Cases</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rent_cases.each do |rent_case| %>
      <tr>
        <td><%= link_to 'Show', user_rent_case_path(rent_case) %></td>
        <td><%= link_to 'Edit', edit_user_rent_case_path(rent_case) %></td>
        <td><%= link_to 'Destroy', user_rent_case_path(rent_case), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rent case', new_user_rent_case_path %>
